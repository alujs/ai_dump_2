{
  "name": "ai_dump_2",
  "version": "1.0.0",
  "description": "Graph-backed MCP controller foundation",
  "homepage": "https://github.com/alujs/ai_dump_2#readme",
  "bugs": {
    "url": "https://github.com/alujs/ai_dump_2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alujs/ai_dump_2.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": ".ai/mcp-controller/src/index.ts",
  "scripts": {
    "build": "echo \"No build step required for source-only MCP controller.\"",
    "dev": "node --import tsx .ai/mcp-controller/src/index.ts",
    "start": "node --import tsx .ai/mcp-controller/src/index.ts",
    "start:mcp": "node --import tsx .ai/mcp-controller/src/mcp/stdioServer.ts",
    "start:mcp:launcher": "node .ai/mcp-controller/scripts/run-mcp-stdio.mjs",
    "test": "tsx --test .ai/mcp-controller/tests/**/*.test.ts",
    "e2e:smoke": "node .ai/mcp-controller/scripts/e2e-smoke.mjs",
    "e2e:mcp-smoke": "node --import tsx .ai/mcp-controller/scripts/e2e-mcp-smoke.mjs",
    "e2e:mcp-stdio-smoke": "bash .ai/mcp-controller/scripts/e2e-mcp-stdio-smoke.sh",
    "verify:isolation": "node .ai/mcp-controller/scripts/verify-isolation.mjs",
    "graphops:check": "node --import tsx .ai/mcp-controller/src/domains/graph-ops/cli.ts check",
    "graphops:sync": "node --import tsx .ai/mcp-controller/src/domains/graph-ops/cli.ts sync",
    "graphops:export": "node --import tsx .ai/mcp-controller/src/domains/graph-ops/cli.ts export"
  },
  "dependencies": {
    "@angular/compiler": "^14.3.0",
    "express": "^4.21.2",
    "neo4j-driver": "^6.0.1",
    "ts-morph": "^23.0.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
